				<div class="signin-form">
					<div class="login-form-rec">
						<form action="#" method="post">
							照護人員證號* <input type="text" name="ID" placeholder="請輸入員工編號" required="">
							請輸入密碼* <input type="password" name="password" id="password1" placeholder="Password" required="">
							再次輸入密碼* <input type="password" name="password" id="password2" placeholder="Confirm Password" required="">
							姓名* <input type="text" name="name" placeholder="請輸入姓名" required="">							
							性別 <select id="country13" onchange="change_country(this.value)" class="frm-field required">
											<option value="null">請選取性別</option>
											<option value="00">男</option>
											<option value="01">女</option>    
							</select>

							年資 <select id="country13" onchange="change_country(this.value)" class="frm-field required">
										<option value="null">請選取年資</option>
										<option value="10">0-1年</option>
										<option value="11">2-4年</option>
										<option value="12">5-7年</option>
										<option value="13">8-10年</option> 
										<option value="14">10年以上</option>    
							</select>

							職位 <select id="country13" multiple onchange="change_country(this.value)" class="frm-field required">
										<option value="null">請選取職位</option>
											<optgroup label="醫院">
												<option value="30">醫師</option>
												<option value="31">護士</option>
												<option value="32">護理長</option>
												<option value="33">科主任</option>
											</optgroup>	    
							</select>
							連絡電話 <input type="text" name="tel" placeholder="請輸入電話" required="">
							電子郵件 <input type="email" name="email" placeholder="請輸入電子郵件" required="">
							<input type="submit" value="註冊">
						</form>
