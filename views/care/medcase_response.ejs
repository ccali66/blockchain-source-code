	<!--/w3_short-->
	<div class="services-breadcrumb_w3layouts">
		<div class="inner_breadcrumb">

			<ul class="short_w3ls"_w3ls>
				<li><a href="/">首頁</a><span>|</span></li>
				<li>調閱病歷結果</li>
			</ul>
		</div>
	</div>
	<div class="container">
        <div class="fontawesome-icon-list">
			<div class="icon-box col-md-3 col-sm-4"><a class="agile-icon" href="http://140.118.9.227:3300/blockchain"><i class="fa fa-location-arrow"></i>信賴程度：</a>
				<% if(riskv=='高'){ %>
					<font color='green' ><%= riskv %></font>
				<% } else if(riskv=='中'){ %>
					<font color='black' ><%= riskv %></font>
				<% } else{ %>
					<font color='red' ><%= riskv %></font>
				<% } %>
			</div>
		</div>
	</div>
	<!--//w3_short-->
	<!-- /inner_content -->
	<div class="inner_content_info_agileits">
		<div class="container">
			<div class="tittle_head_w3ls">
				<h3 class="tittle three">長照人員調閱病歷結果</h3>
			</div>
			<div class="inner_sec_grids_info_w3ls">
				<div class="col-md-8 job_info_left">
					<% 
					if(data.length == 0){
						%>查無調閱病歷紀錄<%
					}else{
						for ( var i = 0 ; i < data.length  ; i++){ %>
							<div class="tab_grid_prof">
								<div class="col-sm-3 loc_1">
									<% var picname =  "images/p"+(i+1)+".png" %>
									<img src='<%= picname %>' >
								</div>
								<div class="col-sm-9">
									<div class="location_box1">
										<h6><%= i+1 %>. 病患名稱 : <%= data[i].PatientName %> </h6>
										<p><span class="m_2"> 病患身分證 : <%= data[i].cardNum %>  </span></p>
										<% if(data[i].Result==1){ %>
											<p><span class="m_2">授權結果 : 審核成功 </span><i class="fa fa-check"> </i> <span class="icon_text"></span></p>
											<p><span class="m_2"><span class="glyphicon glyphicon-time" aria-hidden="true"></span> <span class="glyphicon-class">申請期間 : <%= moment(data[i].createtime).format('YYYY-MM-DD'); %></span></p>
											<p><span class="m_2"><i class="fa fa-times" ></i> <span>截止時間 : <%= moment(data[i].createtime).add(7, 'days').format('YYYY-MM-DD'); %></span></p>
												<% if(moment(data[i].createtime).add(7, 'days').format('YYYY-MM-DD') != moment().format('YYYY-MM-DD')){ %>
												<ul class="links_bottom">										
													<li class="last"><a href= '<%= data[i].file %>' target="_blank"><span class="icon_text">查看 <i class="fa fa-caret-right icon_1"> </i></span></a></li>
												</ul> 
												<% } else{ %>
													<p><span class="m_2">授權時間已過期</span><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></p>
												<% } %>
											<% } else if(data[i].Result==2){ %>
												<p><span class="m_2">授權結果 : 審核中 </span></p>
												<p><span class="m_2"><span class="glyphicon glyphicon-time" aria-hidden="true"></span> <span class="glyphicon-class">申請期間 : <%= moment(data[i].createtime).format('YYYY-MM-DD'); %></span></p>
											<% } else{ %>
											<p><span class="m_2">授權結果 : 審核失敗 </span><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></p>
											<p><span class="m_2"><span class="glyphicon glyphicon-time" aria-hidden="true"></span> <span class="glyphicon-class">申請期間 : <%= moment(data[i].createtime).format('YYYY-MM-DD'); %></span></p>
											<% } %>
									</div>
								</div>
								<div class="clearfix"> </div>
							</div>
						<% }
					}%>
<!--
					<ul class="pagination paging w3-agileits-paging">
						<li>
							<a href="#" aria-label="Previous">
						<span aria-hidden="true">«</span>
					  </a>
						</li>
						<li><a href="#">1</a></li>
						<li><a href="#">2</a></li>
						<li><a href="#">3</a></li>
						<li><a href="#">4</a></li>
