						<li>
							<a href="#" aria-label="Next">
						<span aria-hidden="true">»</span>
					  </a>
						</li>
					</ul>-->
				</div>
				<div class="col-md-4 job_info_right">
					<div class="col_3 experience">
						<h3>目前登入使用者</h3>
						<table class="table">
							<tbody>
								<tr class="unread checked">
									<td class="hidden-xs">
										工作證號:
									</td>
									<td class="hidden-xs">
										<%= user.workID %>
									</td>
								</tr>
								<tr class="unread checked">
									<td class="hidden-xs">
										角色:
									</td>
										<% 
											var attr = user.attr.split(',');
											for(var j = 0;j < attr.length;j++){
												if(j!=0){
													%></tr>
													<tr class="unread checked">
														<td class="hidden-xs">
														</td>
													<%
												}
												switch(attr[j]) {
													case "00":
														%><td class="hidden-xs"> 男 </td><% 
														break; 
													case "01":
														%><td class="hidden-xs"> 女 </td><% 
														break; 
													case "10":
														%><td class="hidden-xs"> 工作經驗:0-1年 </td><%  
														break; 
													case "11": 
														%><td class="hidden-xs"> 工作經驗:2-4年 </td><%  
														break; 
													case "12": 
														%><td class="hidden-xs"> 工作經驗:5-7年 </td><%  
														break; 
													case "13": 
														%><td class="hidden-xs"> 工作經驗:8-10年 </td><%  
														break;
													case "14": 
														%><td class="hidden-xs"> 工作經驗:10年以上 </td><%  
														break;
													case "20": 
														%><td class="hidden-xs"> 照顧服務人員 </td><%  
														break;
													case "21": 
														%><td class="hidden-xs"> 物理治療師 </td><%  
														break;
													case "22": 
														%><td class="hidden-xs"> 職能治療師 </td><%  
														break;
													case "24": 
														%><td class="hidden-xs"> 營養師 </td><%  
														break;
													case "30": 
														%><td class="hidden-xs"> 醫師 </td><%  
														break;
													case "31": 
														%><td class="hidden-xs"> 護士 </td><%  
														break;
													case "40": 
														%><td class="hidden-xs"> 內科 </td><%  
														break;
													case "41": 
														%><td class="hidden-xs"> 外科 </td><%  
														break;
													case "42": 
														%><td class="hidden-xs"> 眼科 </td><%  
														break;
													case "43": 
														%><td class="hidden-xs"> 骨科 </td><%  
														break;
													case "44": 
														%><td class="hidden-xs"> 復健科 </td><%  
														break;
													default: 
														%> <%  
														break;
												}
											} 
										%>
								</tr>
								<!--
								<tr class="unread checked">
									<td class="hidden-xs">
										<input type="checkbox" class="checkbox">
									</td>
									<td class="hidden-xs">
										審核中
									</td>
									<td>
										(1)
									</td>
								</tr>
								<tr class="unread checked">
									<td class="hidden-xs">
										<input type="checkbox" class="checkbox">
									</td>
									<td class="hidden-xs">
										審核失敗
									</td>
									<td>
										(1)
									</td>
								</tr>-->								
							</tbody>
						</table>
					</div>					
				</div>
				<div class="clearfix"></div>
			</div>
		</div>
	</div>
